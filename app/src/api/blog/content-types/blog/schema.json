{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-detail.blog-detail"
    },
    "coverImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
